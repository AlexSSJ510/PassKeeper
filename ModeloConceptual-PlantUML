@startuml
class Usuario {
    - id: String
    - nombre: String
    - email: String
    - contrasena: String
    + Usuario()
    + getId(): String
    + setId(id: String)
    + getNombre(): String
    + setNombre(nombre: String)
    + getEmail(): String
    + setEmail(email: String)
    + getContrasena(): String
    + setContrasena(contrasena: String)
}

class Password {
    - id: String
    - sitioWeb: String
    - usuario: String
    - contrasena: String
    + Password()
    + getId(): String
    + setId(id: String)
    + getSitioWeb(): String
    + setSitioWeb(sitioWeb: String)
    + getUsuario(): String
    + setUsuario(usuario: String)
    + getContrasena(): String
    + setContrasena(contrasena: String)
}

class GestorContraseñas {
    - contraseñas: List<Password>
    + GestorContraseñas()
    + agregarContrasena(contrasena: Password)
    + eliminarContrasena(id: String)
    + buscarContrasena(sitioWeb: String): Password
    + listarContrasenas(): List<Password>
}

class Seguridad {
    + encriptar(contrasena: String): String
    + desencriptar(contrasenaEncriptada: String): String
}

Usuario "1" -- "0..*" Password : tiene
GestorContraseñas "1" -- "0..*" Password : gestiona
Usuario "1" -- "1" GestorContraseñas : usa
@enduml
